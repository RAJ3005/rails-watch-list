<h1>Movies</h1>
<%= link_to 'New List', new_list_path %>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Overview</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= list.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
